{
  "name": "carrying",
  "version": "0.0.1",
  "description": "A library with functional functions that carries you towards happier and better code.",
  "keywords": [
    "functional functions",
    "functional",
    "range",
    "zip",
    "foldl",
    "scan",
    "curry",
    "currying"
  ],
  "main": "dist/carrying.js",
  "scripts": {
    "prepublish": "npm-run-all prepublish:*",
    "prepublish:mkdir-tmp": "mkdir -p .tmp",
    "prepublish:tsdm": "tsdm rewire",
    "prepublish:cp-tsconfig": "cp tsconfig.base.json tsconfig.json",
    "prepublish:tsconfig": "tsconfig -i 2",
    "prepublish:build": "npm run build",
    "lint": "npm run lint:typescript",
    "lint:typescript": "tslint -c tslint.json `find src -name '*.ts'`",
    "build": "rm -rf dist && webpack",
    "watch": "gulp watch --quick",
    "test": "npm-run-all test:*",
    "test:build": "npm run build && webpack --config webpack.tests.config.js",
    "test:unit": "mocha -r babel-polyfill -r source-map-support/register  'dist/*.spec.js'",
    "test-watch": "npm run test:unit -- --watch",
    "generate-docs": "tsc && esdoc -c esdoc.json"
  },
  "author": "Jesper HÃ¥kansson",
  "license": "Apache-2.0",
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-core": "^6.7.7",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.6.0",
    "babel-preset-stage-2": "^6.5.0",
    "chai": "^3.5.0",
    "esdoc": "^0.4.7",
    "esdoc-es7-plugin": "0.0.3",
    "mocha": "^2.4.5",
    "mocha-loader": "^0.7.1",
    "mock-functions": "^0.1.1",
    "npm-run-all": "^1.5.1",
    "retyped-chai-tsd-ambient": "^3.4.0-0",
    "retyped-mocha-tsd-ambient": "^2.2.5-0",
    "retyped-node-tsd-ambient": "^1.5.3-0",
    "source-map-support": "^0.4.0",
    "stack-source-map": "^1.0.5",
    "ts-loader": "^0.8.2",
    "ts-node": "^0.7.2",
    "tsconfig-glob": "^0.4.1",
    "tsdm": "0.1.0-3",
    "tslint": "^3.8.1",
    "typescript": "^1.9.0-dev.20160428-1.0",
    "webpack": "^1.13.0"
  }
}
